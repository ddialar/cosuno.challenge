{
  "name": "cosuno-tech-challenge",
  "version": "0.1.0",
  "description": "Tech challenge included into a FullStack open position process.",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "npx eslint --fix",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hotst npm",
    "deps": "npm run deps:audit && npm run deps:sec-scan",
    "deps:audit": "npm audit --omit=dev && npm outdated -l",
    "deps:sec-scan": "npm run lint:lockfile",
    "test": "jest --no-cache --runInBand",
    "test:watch": "npm run test -- --watch --detectOpenHandles --verbose",
    "release:breaking_change": "npm --no-git-tag-version version major",
    "release:feature": "npm --no-git-tag-version version minor",
    "release:bug": "npm --no-git-tag-version version patch",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "dependencies": {
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "next": "^12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.4",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "lockfile-lint": "^4.8.0",
    "node-mocks-http": "^1.11.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "4.7.4"
  }
}
